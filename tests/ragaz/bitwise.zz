def main():
    var x = 3

    print(~x)
    print(x << 2)
    print(x >> 1)
    print(x & 1)
    print(x | 4)
    print(x ^ 1)

    x = 3
    x <<= 2
    print(x)

    x = 3
    x >>= 1
    print(x)

    x = 3
    x &= 2
    print(x)

    x = 3
    x |= 4
    print(x)

    x = 3
    x ^= 1
    print(x)
